{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ],
      ["minify", {
        "keepFnName": true
      }]
    ],
    "plugins": [
        "@babel/plugin-transform-arrow-functions"
    ],
    "ignore": [
      "**/*.spec.js"
    ]
  }
  